<Window x:Class="Database_conventer.Windows.LogsWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Database_conventer.Windows"
        mc:Ignorable="d"
        Title="Logs" Height="432" Width="337" MinWidth="337" MinHeight="432" MaxHeight="432" MaxWidth="337" >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="93*"/>
            <ColumnDefinition Width="236*"/>
        </Grid.ColumnDefinitions>
        <RichTextBox x:Name="logs_textbox" Height="311" Margin="10,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.505,0.525" HorizontalAlignment="Left" Width="305" IsReadOnly="True" Grid.ColumnSpan="2">
            <FlowDocument>
            </FlowDocument>
        </RichTextBox>
        <Button x:Name="clear_logs_button" Content="Update" HorizontalAlignment="Left" Margin="10,326,0,0" VerticalAlignment="Top" Width="305" Height="30" Click="Clear_logs_button_Click" Grid.ColumnSpan="2"/>
        <Button x:Name="close_window_logs" Content="Close windows" HorizontalAlignment="Left" Margin="10,363,0,0" VerticalAlignment="Top" Width="305" Height="26" Click="Close_window_logs_Click" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
